{
  "root": true,
  "plugins": ["prettier"],
  "extends": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-new": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-alert": "off",
    "no-undef": "off",
    "import/extensions": "off",
    "prefer-template": "off",
    "max-depth": ["error", 2],
    "eol-last": ["error", "always"],
    "prettier/prettier": "error",
    "parser": "babel-eslint"
  }
}
